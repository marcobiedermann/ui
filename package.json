{
  "name": "ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "build-storybook -c .storybook -o docs",
    "develop": "start-storybook -p 9001 -c .storybook",
    "fix:css": "npm run lint:css -- --fix",
    "fix:js": "npm run lint:js -- --fix",
    "fix": "npm run fix:css && npm run fix:js",
    "format": "prettier --write '**/*.{css,html,js,jsx,json,md,yml,yaml}'",
    "lint:css": "stylelint '**/*.css'",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "lint": "npm run lint:css && npm run lint:js",
    "postinstall": "lerna bootstrap",
    "publish": "lerna publish",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcobiedermann/ui.git"
  },
  "keywords": [
    "css",
    "design-system",
    "style",
    "ui"
  ],
  "author": "Marco Biedermann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcobiedermann/ui/issues"
  },
  "homepage": "https://github.com/marcobiedermann/ui#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "lerna": "^3.21.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0"
  },
  "dependencies": {
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-options": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "postcss": "^7.0.30",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
